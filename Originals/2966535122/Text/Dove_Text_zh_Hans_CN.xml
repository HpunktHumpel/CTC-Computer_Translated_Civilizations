	<!-- Leader Pigeon。-->
	<LocalizedText>
		<Row Tag="LOC_LEADER_PIGEON_GIRL_NAME" Language="zh_Hans_CN">
			<Text>鸽子娘</Text>
		</Row>
		<Row Tag="LOC_LEADER_PIGEON_GIRL_QUOTE" Language="zh_Hans_CN">
			<Text>人生苦短，及时行乐。</Text>
		</Row>
		<Row Tag="LOC_TRAIT_LEADER_WITHOUT_SUPERVISION_NAME" Language="zh_Hans_CN">
