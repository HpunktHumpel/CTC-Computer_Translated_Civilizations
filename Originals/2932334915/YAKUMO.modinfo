  <Properties>
    <Name>TOUHOU YAKUMO</Name>
    <Description>TOUHOU YAKUMO</Description>
    <Created>1676212317</Created>
    <Teaser>This example mod demonstrates how to add a new leader and civilization to the game.</Teaser>
    <Authors>JIDONG</Authors>
    <CompatibleVersions>1.2,2.0</CompatibleVersions>
  </Properties>
  <ActionCriteria>
    <Criteria id="LeaderCriteria">
      <LeaderPlayable>Players:StandardPlayers::LEADER_JD_YUKARI,Players:StandardPlayers::LEADER_JD_RAN,
    	Players:Expansion1_Players::LEADER_JD_YUKARI,Players:Expansion1_Players::LEADER_JD_RAN,
    	Players:Expansion2_Players::LEADER_JD_YUKARI,Players:Expansion2_Players::LEADER_JD_RAN</LeaderPlayable>
    </Criteria>
    <Criteria id="LeaderCriteria_Expansion1">
      <LeaderPlayable>
    	Players:Expansion1_Players::LEADER_JD_YUKARI,Players:Expansion1_Players::LEADER_JD_RAN</LeaderPlayable>
      <RuleSetInUse>RULESET_EXPANSION_1</RuleSetInUse>
    </Criteria>
    <Criteria id="LeaderCriteria_Expansion2">
      <LeaderPlayable>
    	Players:Expansion2_Players::LEADER_JD_YUKARI,Players:Expansion2_Players::LEADER_JD_RAN</LeaderPlayable>
      <RuleSetInUse>RULESET_EXPANSION_2</RuleSetInUse>
    </Criteria>
  </ActionCriteria>
