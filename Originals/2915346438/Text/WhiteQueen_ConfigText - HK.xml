		<Replace Tag="LOC_GOVERNOR_HIBIKI_TITLE" Language="zh_Hant_HK">
			<Text>空無</Text>
		</Replace>
		<Replace Tag="LOC_GOVERNOR_HIBIKI_DESCRIPTION" Language="zh_Hant_HK">
