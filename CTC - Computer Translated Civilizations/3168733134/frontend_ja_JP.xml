		<Replace Tag="LOC_BUILDING_PEN_FEITARMIAO_TEMPLE_NAME" Language="ja_JP">
			<Text>ポセイドン神殿</Text>
		</Replace>
		<Replace Tag="LOC_BUILDING_PEN_FEITARMIAO_TEMPLE_DESCRIPTION" Language="ja_JP">
