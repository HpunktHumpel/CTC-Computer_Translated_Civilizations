		</Replace>
		<Replace Tag="LOC_DIPLO_DECLARE_FRIEND_FROM_AI_LEADER_SUNAO_ANY" Language="en_US">
			<Text>Willing to accept my friendship</Text>
		</Replace>
		<Replace Tag="LOC_DIPLO_ACCEPT_DECLARE_FRIEND_FROM_AI_LEADER_SUNAO_ANY" Language="en_US">
