		</Replace>
		<Replace Tag="LOC_DIPLO_DEFEAT_FROM_AI_LEADER_SHIZUKA_ANY" Language="en_US">
			<Text>I still don't understand my pride</Text>
		</Replace>
		<Replace Tag="LOC_DIPLO_ACCEPT_MAKE_DEAL_FROM_AI_LEADER_SHIZUKA_ANY" Language="en_US">
			<Text>very good</Text>
		</Replace>
		<Replace Tag="LOC_DIPLO_REJECT_MAKE_DEAL_FROM_AI_LEADER_SHIZUKA_ANY" Language="en_US">
			<Text>All right</Text>
		</Replace>
		<Replace Tag="LOC_DIPLO_ACCEPT_DECLARE_FRIEND_FROM_HUMAN_LEADER_SUNAO_ANY" Language="en_US">
